#: lib/wxs-settings.php:183 lib/wxs-settings.php:197
msgid "Basic Settings"
msgstr "基础设置"

#: lib/wxs-settings.php:204
msgid "Enable Blind Watermark"
msgstr "启用盲水印"

#: lib/wxs-settings.php:205
msgid "When enabled, blind watermarks will be inserted into article content"
msgstr "开启后将在文章内容中插入盲水印"

#: lib/wxs-settings.php:213
msgid "Run Mode"
msgstr "运行模式"

#: lib/wxs-settings.php:215
msgid "Dynamic (Pure PHP)"
msgstr "动态（纯PHP）"

#: lib/wxs-settings.php:216
msgid "Static (Pure JS)"
msgstr "静态（纯JS）"

#: lib/wxs-settings.php:217
msgid "Dynamic-Static Hybrid"
msgstr "动静混合"

#: lib/wxs-settings.php:221
msgid "Dynamic: Pure PHP processing, recommended, ensures no page caching plugins like super cache are installed<br>Static: Pure JS processing, not highly recommended, can be bypassed<br>Dynamic-Static Hybrid: PHP for logged-in users, JS for non-logged-in users (suitable for websites with caching)<br>If JS mode or hybrid mode is enabled, please enable WAF to block fake crawlers"
msgstr "动态：纯PHP处理，推荐，确保没有安装如super cache页面缓存类似插件<br>静态：纯JS处理，不是很推荐，可被绕过<br>动静混合：登录用户用PHP，未登录用户用JS（适合有缓存的网站）<br>若开启JS模式或混合模式下，请开启WAF拦截假蜘蛛"

#: lib/wxs-settings.php:231
msgid "Minimum Paragraph Length"
msgstr "最小段落字数"

#: lib/wxs-settings.php:232
msgid "Paragraphs with fewer than this number of characters will not have watermarks inserted (recommended 15-30)"
msgstr "少于此字数的段落不插入水印（建议15-30）"

#: lib/wxs-settings.php:242
msgid "Insertion Method"
msgstr "插入方式"

#: lib/wxs-settings.php:244
msgid "Insert at Paragraph End"
msgstr "段落末尾插入"

#: lib/wxs-settings.php:245
msgid "Insert at Random Positions"
msgstr "随机位置插入"

#: lib/wxs-settings.php:246
msgid "Insert at Fixed Character Intervals"
msgstr "固定字数插入"

#: lib/wxs-settings.php:249
msgid "Select how watermarks are inserted into articles"
msgstr "选择水印在文章中的插入方式"

#: lib/wxs-settings.php:256
msgid "Random Position Insertion Settings"
msgstr "随机位置插入设置"

#: lib/wxs-settings.php:265
msgid "Insertion Count Mode"
msgstr "插入次数模式"

#: lib/wxs-settings.php:267
msgid "Custom Count"
msgstr "自定义次数"

#: lib/wxs-settings.php:268
msgid "Auto-calculate by Word Count"
msgstr "按字数自动计算"

#: lib/wxs-settings.php:279
msgid "Custom Insertion Count"
msgstr "自定义插入次数"

#: lib/wxs-settings.php:280
msgid "Fixed number of watermark insertions per paragraph"
msgstr "每段固定插入的水印次数"

#: lib/wxs-settings.php:292
msgid "Word Count Ratio"
msgstr "字数比例"

#: lib/wxs-settings.php:293
msgid "Add 1 insertion per how many words (e.g., 400 = 1 insertion per 400 words)"
msgstr "每多少字增加1次插入（例：400=每400字插入1次）"

#: lib/wxs-settings.php:306
msgid "Fixed Character Interval Insertion Settings"
msgstr "固定字数插入设置"

#: lib/wxs-settings.php:315
msgid "Insertion Interval"
msgstr "插入间隔"

#: lib/wxs-settings.php:316
msgid "Insert watermark every how many characters"
msgstr "每多少字插入1次水印"

